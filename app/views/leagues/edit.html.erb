<% provide(:title, "Update league") %>
<h1>Update league</h1>
<div class="row">
  <div class="span6 offset3">
    <%= form_for(@league) do |f| %>
      <%= render 'shared/error_messages', object: f.object %>

      <%= f.label :name %>
      <%= f.text_field :name %>

      <%= f.submit "Update league", class: "btn btn-large btn-primary" %>
    <% end %>
  </div>
</div>